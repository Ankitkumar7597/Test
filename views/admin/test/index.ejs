    <div class="row">
        <div class="col-md-12">
            <div class="dashbord-bac-main mb-6 mt-3">
                <div class="card-body mb-0 pb-0">
                    <div class="row justify-content-end justify-content-md-between">
                        <div class="form-group col-md-3 order-2 order-md-1">
                            <select id="eventStatus"
                                class="select2 form-control in_field js-example-basic-single select2-hidden-accessible">
                                <option selected value="all">All</option>
                                <option value="0">Inactive</option>
                                <option value="1">Active</option>
                            </select>
                        </div>
                        <div class="form-group ml-2 w-auto order-1 order-md-2">
                            <a class="effect-btn btn squer-btn  text-white openPopupModel" data-bs-target="#eventModal" dataBody="eventModal_body"
                                dataTitle="Add Form" dataModelId="eventModal" href="javascript:void(0);"
                                dataHref="<%= admin %>/add-from/0" role="button" data-bs-toggle="modal">Add Form</a>
                        </div>
                    </div>
                    <div class="table-responsive pt-3">
                        <table id="testList" class="table20 table-striped table-bordered main-table-class"
                            style="width:100%">
                            <thead>
                                <tr class="main-tr-152r">
                                    <th class="table-th-class-1">#</th>
                                    <th class="table-th-class-1">Name</th>
                                    <th class="table-th-class-1">Mobile Number</th>
                                    <th class="table-th-class-1">Address</th>
                                    <th class="table-th-class-1">Office Adress</th>
                                    <th class="table-th-class-1">Creation Date</th>
                                    <th class="table-th-class-1">Status</th>
                                    <th class="table-th-class-1">Action</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                    <div id="msg"></div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="eventModal" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-frame modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title"></h5>
                <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close"><span
                        aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body p-0 pt-2 eventModal_body">

            </div>
        </div>
    </div>
</div>